config global 
    option anon_swap '0'
    option anon_mount '0'
    option auto_swap '1'
    option auto_mount '1'
    option delay_root '5'
    option check_fs '0'

config mount
    option target '/sd'
    option device '/dev/sdb1'
    option fstype 'auto'
    option options 'rw,sync'
    option enabled '1'
    
config swap
	option device '/dev/sdb2'
	option enabled '1'
